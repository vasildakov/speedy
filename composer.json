{
    "name": "vasildakov/speedy",
    "description": "Speedy API Client",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "VasilDakov\\Speedy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VasilDakov\\SpeedyTest\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Vasil Dakov",
            "email": "vasildakov@gmail.com",
            "homepage": "https://github.com/vasildakov",
            "role": "Developer"
        },
        {
            "name": "Valentin Valkanov",
            "email": "valentinvalkanof@gmail.com",
            "homepage": "https://github.com/valentin-valkanov",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4 || ~8.1.0",
        "ext-curl": "*",
        "ext-json": "*",
        "beberlei/assert": "dev-master",
        "fig/http-message-util": "1.1.x-dev",
        "laminas/laminas-diactoros": "2.15.x-dev",
        "php-http/curl-client": "2.x-dev",
        "guzzlehttp/psr7": "2.4.x-dev",
        "php-http/message": "^1.13",
        "php-http/discovery": "^1.14",
        "guzzlehttp/guzzle": "7.4.x-dev",
        "vlucas/phpdotenv": "5.4.x-dev",
        "doctrine/collections": "2.0.x-dev",
        "laminas/laminas-hydrator": "4.6.x-dev",
        "laminas/laminas-serializer": "2.14.x-dev",
        "jms/serializer": "3.x-dev",
        "doctrine/cache": "2.2.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "php-coveralls/php-coveralls": "dev-master",
        "squizlabs/php_codesniffer": "4.0.x-dev",
        "theseer/phpdox": "dev-master"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --coverage-html ./build/coverage"
    }
}
